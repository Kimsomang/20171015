-- ZIPCODE_OLD 테이블 생성
CREATE TABLE ZIPCODE_OLD( 
	ZIPCODE VARCHAR2(7) not null, 
	SIDO VARCHAR2(4), 
	GUGUN VARCHAR2(17), 
	DONG VARCHAR2(26), 
	RI VARCHAR2(15), 
	BLDG VARCHAR2(42), 
	BUNJI VARCHAR2(17), 
	SEQ NUMBER(5) not null
);

-- 제약조건 추가
ALTER TABLE ZIPCODE_OLD ADD CONSTRAINT PK_ZIPCODE_OLD_SEQ primary key(SEQ);